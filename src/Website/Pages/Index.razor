@page "/"

<PageTitle>Index</PageTitle>

<Field Name="Username"
       InputType="Enums.InputType.Text"
       InputOnePlaceholder="Username"
       HasIconLabel="false"
       LocalizedLabel="Username"
       LocalizedDescription="Please enter your username" />

<Field Name="Password"
       InputType="Enums.InputType.Password"
       InputOnePlaceholder="Password"
       LocalizedLabel="Password"
       LocalizedDescription="Please enter your Password"
       LocalizedRequiredError="DividerType is test"
       HasIconLabel="true"
       IconLabel="@($"@")"
       LabelType="LabelType.Before" />


<Field Name="Password"
       InputType="Enums.InputType.Password"
       InputOnePlaceholder="Password"
       LocalizedLabel="Password"
       LocalizedDescription="Please enter your Password"
       LocalizedRequiredError="DividerType is test"
       HasIconLabel="true"
       IconLabel="@($"@")"
       LabelType="LabelType.Before" />

<Field Name="Password"
       InputType="Enums.InputType.Text"
       InputOnePlaceholder="Password"
       LocalizedLabel="Password"
       LocalizedDescription="Please enter your Password"
       LocalizedRequiredError="DividerType is test"
       HasIconLabel="true"
       IconLabel="@($"@")"
       LabelType="LabelType.After" />


<MudButton OnClick="@(() => Test())">Test</MudButton>

@code {
    public string Username { get; set; }
    public string Password { get; set; }


    void Test()
    {

    }
